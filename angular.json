      }
    },
    "e2e-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/e2e-app",
      "sourceRoot": "projects/e2e-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/e2e-app",
            "index": "projects/e2e-app/src/index.html",
            "main": "projects/e2e-app/src/main.ts",
            "polyfills": "projects/e2e-app/src/polyfills.ts",
            "tsConfig": "projects/e2e-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/e2e-app/src/favicon.ico",
              "projects/e2e-app/src/assets"
            ],
            "styles": [
              "node_modules/@ajf/icons/css/ajf-icons.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/@ionic/angular/css/ionic.bundle.css",
              "node_modules/flag-icon-css/css/flag-icons.min.css",
              "node_modules/leaflet/dist/leaflet.css",
              "projects/e2e-app/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "chart.js",
              "dagre",
              "svg-pan-zoom",
              "pdfmake/build/pdfmake"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "20kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "e2e-app:build:production"
            },
            "development": {
              "browserTarget": "e2e-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
